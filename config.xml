  <concurrentBuild>false</concurrentBuild>
  <builders>
    <hudson.tasks.Shell>
      <command>#!/bin/bash
set -xe
rm -fr drcutil
git clone --branch jenkins --single-branch https://github.com/jenkinshrg/drcutil.git drcutil
cd drcutil
source .jenkins.sh task wall</command>
    </hudson.tasks.Shell>
